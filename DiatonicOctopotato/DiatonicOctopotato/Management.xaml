        <Button x:Name="button1" Content="Select List" HorizontalAlignment="Left" Height="22" Margin="193,10,0,0" VerticalAlignment="Top" Width="89"/>
        <Button x:Name="button1_Copy" Content="New List" HorizontalAlignment="Left" Height="22" Margin="193,37,0,0" VerticalAlignment="Top" Width="89"/>
