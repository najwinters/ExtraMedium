<Window x:Class="DiatonicOctopotato.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiatonicOctopotato"
        mc:Ignorable="d"
        Title="Term Input" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="termTB" HorizontalAlignment="Left" Height="50" Margin="10,52,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="272"/>
        <TextBox x:Name="defTB" HorizontalAlignment="Left" Height="50" Margin="10,133,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="272"/>
        <Label x:Name="label" Content="Term:" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top" Width="46"/>

    </Grid>
</Window>
